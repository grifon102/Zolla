@model IEnumerable<Zolla.Data.Entities.Order>

@{
    ViewBag.Title = "Index";
}

@section scripts{
    <script type="text/javascript">
        $.ajax({
            url: '@Url.Action("Index","Orders")',
            type: 'GET',
            contentType: 'application/html; charset=utf-8',
            success: function (data) {
                $('#cont').html(data);
            },
            error: function (xhr, status) {
                alert(status);
            }
        });
    </script>
    }

@Html.Partial("_Orders",Model)

